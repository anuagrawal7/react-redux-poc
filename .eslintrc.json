{
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 1,
        "no-trailing-spaces": "error",
        "indent": [2, 2, { "SwitchCase": 1 }],
        "no-debugger": 1,
        "key-spacing": [2, {"afterColon": true, "beforeColon": false}]
    }
}